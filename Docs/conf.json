{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [ "../src/Phaser.js", "../src/animation/Animation.js" ],
        "exclude": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./out/",
        "recurse": true,
        "private": false,
        "lenient": true,
    }
}